    while (not place_meeting(x + sign(moveX), y - 2, obj_ground)) {
        x += sign(moveX)
    }
    moveX = 0
}

// Sprite Check
if (input_left + input_right != 0 and input_left + input_right != 2) {
    image_xscale = sign(input_right - input_left)
}

// Apply Movement
x += moveX
y += moveY
